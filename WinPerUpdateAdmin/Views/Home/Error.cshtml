@using System.Web.Optimization;
@{
    ViewBag.Title = "Error";
}
@Scripts.Render("~/bundles/login")
<div class="container">
    <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-6">
            <center>
                <img src="~/Content/Images/http_caution.png" alt="" />
            </center>
        </div>
        <div class="col-md-3"></div>
    </div>
    <br />
    <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-6">
            <div class="alert alert-danger">
                <strong>1. </strong>No ha iniciado sesión.
            </div>
            <div class="alert alert-danger">
                <strong>2. </strong>No es un usuario valido o no existe en nuestra base de datos.
            </div>
            <div class="alert alert-danger">
                <strong>3. </strong>No tiene permiso para ver la página solicitada.
            </div>
        </div>
        <div class="col-md-3"></div>
    </div>
</div>

<input type="hidden" id="idToken" value="@Session["token"]" />